{
  "name": "resium",
  "version": "1.10.0",
  "description": "React components for Cesium",
  "main": "dist/resium.cjs.js",
  "module": "dist/resium.es.js",
  "types": "dist/types/src/index.d.ts",
  "repository": "https://github.com/darwin-education/resium.git",
  "author": "rot1024 <aayhrot@gmail.com>",
  "license": "MIT",
  "sideEffects": false,
  "scripts": {
    "test": "jest --coverage",
    "clean": "del dist coverage examples/build",
    "clean:build": "del dist",
    "type": "tsc --noEmit",
    "lint": "eslint --ext '.js,.ts,.tsx' .",
    "build": "run-s clean:build build:es build:cjs build:umd build:umd:min",
    "build:es": "cross-env NODE_ENV=es rollup -c",
    "build:cjs": "cross-env NODE_ENV=cjs rollup -c",
    "build:umd": "rollup -c",
    "build:umd:min": "cross-env NODE_ENV=production rollup -c",
    "format": "eslint --ext '.js,.ts,.tsx' --quiet --fix .",
    "storybook": "start-storybook -s .storybook/public -p 9001",
    "storybook:build": "build-storybook -s .storybook/public -o .docz/dist/examples",
    "doc": "node docs/scripts/generateDoc && docz dev",
    "doc:build": "node docs/scripts/generateDoc && docz build && build-storybook -o .docz/dist/examples",
    "doc:generate": "node docs/scripts/generateDoc"
  },
  "files": [
    "src",
    "dist"
  ],
  "engines": {
    "node": ">=8.3.0"
  },
  "peerDependencies": {
    "cesium": "1.x",
    "react": ">=16.8",
    "react-dom": ">=16.8"
  },
  "devDependencies": {
    "@babel/core": "7.7.7",
    "@emotion/core": "10.0.27",
    "@emotion/styled": "10.0.27",
    "@rollup/plugin-replace": "2.3.0",
    "@storybook/addon-actions": "5.2.8",
    "@storybook/addon-storysource": "5.2.8",
    "@storybook/react": "5.2.8",
    "@storybook/theming": "5.2.8",
    "@types/cesium": "1.59.5",
    "@types/enzyme": "3.10.4",
    "@types/jest": "24.0.25",
    "@types/lodash.omit": "4.5.6",
    "@types/lodash.pick": "4.4.6",
    "@types/react": "16.9.17",
    "@types/react-dom": "16.9.4",
    "@typescript-eslint/eslint-plugin": "2.14.0",
    "@typescript-eslint/parser": "2.14.0",
    "babel-loader": "8.0.6",
    "cesium": "1.64.0",
    "copy-webpack-plugin": "5.1.1",
    "cross-env": "6.0.3",
    "del-cli": "3.0.0",
    "docz": "1.3.2",
    "docz-theme-default": "1.2.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.9.0",
    "eslint-plugin-node": "11.0.0",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.17.0",
    "eslint-plugin-react-hooks": "2.3.0",
    "file-loader": "5.0.2",
    "globby": "10.0.1",
    "jasmine-core": "3.5.0",
    "jest": "24.9.0",
    "lodash.omit": "4.5.0",
    "lodash.pick": "4.4.0",
    "npm-run-all": "4.1.5",
    "prettier": "1.19.1",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "rollup": "1.27.14",
    "rollup-plugin-cleanup": "3.1.1",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-sourcemaps": "0.4.2",
    "rollup-plugin-terser": "5.1.3",
    "rollup-plugin-typescript2": "0.25.3",
    "ts-jest": "24.2.0",
    "ts-loader": "6.2.1",
    "typescript": "3.7.4",
    "webpack": "4.41.5"
  }
}
